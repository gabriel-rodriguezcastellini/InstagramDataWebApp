@model List<InstagramDataWebApp.Models.InstagramRelationship>
@{
    ViewData["Title"] = "Following";
}

<h1>Following</h1>

<table border="1" cellpadding="5">
    <thead>
        <tr>
            <th>Username</th>
            <th>Profile Link</th>
            <th>Timestamp</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            var link = item.StringListData?.FirstOrDefault();
            if (link != null)
            {
                <tr>
                    <td>@link.Value</td>
                    <td><a href="@link.Href" target="_blank">@link.Href</a></td>
                    <td>@link.Timestamp</td>
                </tr>
            }
        }
    </tbody>
</table>
